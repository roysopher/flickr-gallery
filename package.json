{
  "name": "flickr-gallery",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "private": true,
  "author": {
    "name": "Guy Sopher",
    "email": "guyso@wix.com"
  },
  "scripts": {
    "start": "haste start",
    "precommit": " ",
    "build": "haste build",
    "pretest": "haste build",
    "test": "haste test",
    "posttest": "haste lint",
    "release": "haste release",
    "publish": "haste build && gh-pages-deploy",
    "clean-source": "rimraf README.md src dist node_modules target test velocity.data.json package.json"
  },
  "devDependencies": {
    "chai": "~4.1.0",
    "enzyme": "~2.9.0",
    "express": "~4.15.0",
    "express-session": "~1.15.0",
    "haste-preset-yoshi": "latest",
    "husky": "~0.14.0",
    "jsdom": "~11.2.0",
    "jsdom-global": "~3.0.0",
    "react-test-renderer": "~15.6.0",
    "velocity": "~0.7.0"
  },
  "dependencies": {
    "axios": "~0.16.0",
    "gh-pages-auto-release": "^1.1.10",
    "gh-pages-deploy": "^0.4.2",
    "i18next": "~8.4.0",
    "i18next-xhr-backend": "~1.4.0",
    "prop-types": "~15.6.0",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-i18next": "~4.8.0",
    "regenerator-runtime": "^0.11.0",
    "wix-axios-config": "latest"
  },
  "haste": {
    "preset": "yoshi"
  },
  "yoshi": {
    "externals": {
      "react": "React",
      "react-dom": "ReactDOM"
    },
    "servers": {
      "cdn": {
        "port": 3200,
        "dir": "dist/statics",
        "ssl": false
      }
    }
  },
  "babel": {
    "presets": [
      "wix"
    ]
  },
  "eslintConfig": {
    "extends": "wix/react"
  },
  "gh-pages-deploy": {
    "staticpath": "dist/statics",
    "post": [
      "clean-source"
    ],
    "noprompt": false
  }
}
